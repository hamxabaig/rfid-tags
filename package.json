{
  "name": "UzysHapiReactBoilerplate",
  "version": "0.1.0",
  "description": "Hapi.js + React.js + Redux Application Boilerplate",
  "main": "app.js",
  "scripts": {
    "start": "pm2 start process.json",
    "test": "lab -C -c -m 2000 -I __core-js_shared__",
    "debug": "./node_modules/devtool/bin/index.js app.js",
    "dev": "export NODE_ENV=development && unset API_ONLY &&node app.js",
    "devapi": "export NODE_ENV=development && export API_ONLY=true &&nodemon -e js,html app.js",
    "build": "rm -rf public && mkdir public && NODE_ENV=production webpack --progress --colors --display-error-details --config webpack.client.js",

    "stop": "pm2 stop process.json",
    "coverage" : "if test -f './converage.html'; then  rm coverage.html; fi && lab -r html -o coverage.html",
    "dep": "npm list --depth 0",
    "outdated": "npm outdated --depth 0"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uzysjung/UzysHapiSkeleton.git"
  },
  "keywords": [
    "Uzys",
    "Hapi",
    "Hapijs",
    "Hapi.js",
    "Node",
    "Node.js",
    "Skeleton",
    "biolerplate",
    "react",
    "redux",
    "react-redux-router"
  ],
  "engines" : { "node" : ">=5.0.0" },
  "dependencies": {
